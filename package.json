{
  "name": "siftz",
  "version": "1.0.0",
  "description": "music connoisseur app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaio0413/SIFTZ.git"
  },
  "author": "",
